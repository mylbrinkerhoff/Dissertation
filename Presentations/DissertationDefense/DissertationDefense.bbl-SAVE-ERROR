      \strng{fullhashraw}{8302fa658b038a406c26db0f6760e735}
      \strng{bibnamehash}{8302fa658b038a406c26db0f6760e735}
      \strng{authorbibnamehash}{8302fa658b038a406c26db0f6760e735}
      \strng{authornamehash}{8302fa658b038a406c26db0f6760e735}
