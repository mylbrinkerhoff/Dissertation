      \strng{namehash}{e7c6e532fa3d5a4779c5a6954f936c2a}
      \strng{fullhash}{e7c6e532fa3d5a4779c5a6954f936c2a}
      \strng{fullhashraw}{e7c6e532fa3d5a4779c5a6954f936c2a}
      \strng{bibnamehash}{e7c6e532fa3d5a4779c5a6954f936c2a}
      \strng{authorbibnamehash}{e7c6e532fa3d5a4779c5a6954f936c2a}
