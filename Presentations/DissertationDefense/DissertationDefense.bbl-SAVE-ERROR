      \strng{namehash}{6a1d1148abd751fc76db7d8261286787}
      \strng{fullhash}{6a1d1148abd751fc76db7d8261286787}
      \strng{fullhashraw}{6a1d1148abd751fc76db7d8261286787}
      \strng{bibnamehash}{6a1d1148abd751fc76db7d8261286787}
      \strng{authorbibnamehash}{6a1d1148abd751fc76db7d8261286787}
