      \strng{bibnamehash}{db6687c2c06dba7eed4d8683d6211ea4}
      \strng{authorbibnamehash}{db6687c2c06dba7eed4d8683d6211ea4}
      \strng{authornamehash}{e0872a4daed3192a96aefbd43d36b117}
      \strng{authorfullhash}{db6687c2c06dba7eed4d8683d6211ea4}
