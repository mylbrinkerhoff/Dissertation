      \field{issn}{0892-1997}
      \field{journaltitle}{Journal of Voice}
      \field{month}{1}
      \field{number}{1}
      \field{shortjournal}{Journal of Voice}
      \field{title}{Objective {{Characterization}} of {{Phonation Type Using Amplitude}} of {{Flow Glottogram Pulse}} and of {{Voice Source Fundamental}}}
      \field{volume}{36}
      \field{year}{2022}
      \field{dateera}{ce}
      \field{pages}{4\bibrangedash 14}
      \range{pages}{11}
