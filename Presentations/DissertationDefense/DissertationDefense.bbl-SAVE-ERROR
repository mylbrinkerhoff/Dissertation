      \field{issn}{0095-4470}
      \field{journaltitle}{Journal of Phonetics}
      \field{langid}{english}
      \field{month}{5}
      \field{number}{3}
      \field{shortjournal}{Journal of Phonetics}
      \field{title}{An Acoustic and Electroglottographic Study of {{White Hmong}} Tone and Phonation}
      \field{volume}{40}
      \field{year}{2012}
      \field{dateera}{ce}
      \field{pages}{466\bibrangedash 476}
      \range{pages}{11}
      \verb{doi}
