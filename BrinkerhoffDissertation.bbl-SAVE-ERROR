      \strng{namehash}{1e4d4f8ec5670b0d097f01128e8654d9}
      \strng{fullhash}{1e4d4f8ec5670b0d097f01128e8654d9}
      \strng{fullhashraw}{1e4d4f8ec5670b0d097f01128e8654d9}
