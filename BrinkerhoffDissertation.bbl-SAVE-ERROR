    \entry{soskuthyGeneralisedAdditiveMixed2017}{online}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=198dc4d6cb4194daec385a3d561f62d7}{%
           family={Sóskuthy},
           familyi={S\bibinitperiod},
           given={Márton},
           giveni={M\bibinitperiod},
           givenun=0}}%
      }
      \strng{namehash}{198dc4d6cb4194daec385a3d561f62d7}
      \strng{fullhash}{198dc4d6cb4194daec385a3d561f62d7}
      \strng{fullhashraw}{198dc4d6cb4194daec385a3d561f62d7}
      \strng{bibnamehash}{198dc4d6cb4194daec385a3d561f62d7}
      \strng{authorbibnamehash}{198dc4d6cb4194daec385a3d561f62d7}
      \strng{authornamehash}{198dc4d6cb4194daec385a3d561f62d7}
