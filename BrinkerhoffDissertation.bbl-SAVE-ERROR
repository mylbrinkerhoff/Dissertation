    \entry{remezIntonationSinusoidalSentences1993}{article}{}{}
      \name{author}{2}{}{%
        {{un=0,uniquepart=base,hash=26f5e7ac270c2b3ba160353dc9088356}{%
           family={Remez},
           familyi={R\bibinitperiod},
