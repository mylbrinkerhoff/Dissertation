      \strng{namehash}{3144096b40a28e6df64cae12c713f49b}
      \strng{fullhash}{3144096b40a28e6df64cae12c713f49b}
      \strng{fullhashraw}{3144096b40a28e6df64cae12c713f49b}
      \strng{bibnamehash}{3144096b40a28e6df64cae12c713f49b}
      \strng{authorbibnamehash}{3144096b40a28e6df64cae12c713f49b}
      \strng{authornamehash}{3144096b40a28e6df64cae12c713f49b}
