    \entry{delvauxPerceptionContrasteNasalite2009}{article}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=c6705fe7fd81012bc4d8e09184763868}{%
           family={Delvaux},
           familyi={D\bibinitperiod},
