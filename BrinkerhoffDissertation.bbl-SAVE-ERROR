    \entry{chaiPhoneticsGlottalizedPhonations2023}{inproceedings}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=fcf466e8710c65fda6efd2698765eb05}{%
           family={Chai},
           familyi={C\bibinitperiod},
