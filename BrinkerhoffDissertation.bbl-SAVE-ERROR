      \strng{authorbibnamehash}{977cbec28aa6156843f5b29d616e8ee3}
      \strng{authornamehash}{f8ae700b02a8a790ef632f7e6395eb02}
      \strng{authorfullhash}{977cbec28aa6156843f5b29d616e8ee3}
