    \entry{mittalStudyEffectsVocal2014}{article}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=823e276299167a690f983b35f6f549bd}{%
           family={Mittal},
           familyi={M\bibinitperiod},
           given={Vinay\bibnamedelima Kumar},
           giveni={V\bibinitperiod\bibinitdelim K\bibinitperiod},
