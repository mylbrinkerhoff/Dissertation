    \entry{warrenQualityQuantityNew2018}{article}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=3483acc53d98318717a517e200c4ecc8}{%
           family={Warren},
           familyi={W\bibinitperiod},
           given={Paul},
           giveni={P\bibinitperiod},
