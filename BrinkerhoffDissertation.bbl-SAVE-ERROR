    \entry{nyczBestPracticesMeasuring2014}{article}{}{}
      \name{author}{2}{}{%
        {{un=0,uniquepart=base,hash=2f1e19f53c69fb95c060776670bca72f}{%
           family={Nycz},
           familyi={N\bibinitperiod},
