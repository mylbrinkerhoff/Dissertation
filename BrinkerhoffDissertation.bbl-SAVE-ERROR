      \strng{fullhashraw}{f9d6ecf3e556b7347013871e0c262b5a}
      \strng{bibnamehash}{f9d6ecf3e556b7347013871e0c262b5a}
      \strng{authorbibnamehash}{f9d6ecf3e556b7347013871e0c262b5a}
      \strng{authornamehash}{b6e95918fc7c17ad51c376d553a4569f}
      \strng{authorfullhash}{f9d6ecf3e556b7347013871e0c262b5a}
      \strng{authorfullhashraw}{f9d6ecf3e556b7347013871e0c262b5a}
