    \entry{lottoEffectVoiceQuality1997}{article}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=f83593e49a0bf53e1d2530b50986a793}{%
           family={Lotto},
           familyi={L\bibinitperiod},
