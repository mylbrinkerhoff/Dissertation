      \field{journaltitle}{Journal of Phonetics}
      \field{langid}{english}
      \field{month}{7}
      \field{number}{3}
      \field{shortjournal}{Journal of Phonetics}
      \field{title}{The Production and Perception of Laryngealized Vowels in {{Coatzospan Mixtec}}}
