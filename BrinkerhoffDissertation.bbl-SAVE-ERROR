      \strng{namehash}{8e89f079dbcace9413bddc693359108d}
      \strng{fullhash}{8e89f079dbcace9413bddc693359108d}
      \strng{fullhashraw}{8e89f079dbcace9413bddc693359108d}
      \strng{bibnamehash}{8e89f079dbcace9413bddc693359108d}
      \strng{authorbibnamehash}{8e89f079dbcace9413bddc693359108d}
